{
  "manifest_version": 2,

  "name": "LinkCount",
  "description": "A simple, fast extension to find the number of internal links on a given webpage.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Get number of links"
  },
  "permissions": [
    "tabs",
    "http://*/*/",
    "https://*/*"
  ]
}
